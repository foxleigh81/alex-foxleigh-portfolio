@font-face {
  font-family: 'icomoon';
  src:  url('../fonts/icomoon.eot?vvquz1');
  src:  url('../fonts/icomoon.eot?vvquz1#iefix') format('embedded-opentype'),
    url('../fonts/icomoon.ttf?vvquz1') format('truetype'),
    url('../fonts/icomoon.woff?vvquz1') format('woff'),
    url('../fonts/icomoon.svg?vvquz1#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="u-icon-"], [class*=" u-icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
